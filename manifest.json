{
  "manifest_version": 2,
  "name": "Image Hosting",
  "description": "图床服务",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "file:///*/*"
      ],
      "css": [
        "dist/style.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "src/img/icon4.png",
    "default_popup": "index.html"
  }
}