{
    "manifest_version": 2,
    "name": "Image Hosting",
    "description": "基于七牛云的免费图床",
    "version": "0.0.1",
    "icons": {
        "16": "assets/imgs/icon16.png",
        "48": "assets/imgs/icon48.png",
        "128": "assets/imgs/icon128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "cookies",
        "storage",
        "tabs",
        "notifications",
        "https://api.jue.sh/*"
    ],
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}